{
  "include": ["src/**/*.ts"],
  "compilerOptions": {
	"baseUrl": ".",
	"declaration": false,
	"esModuleInterop": true,
	"experimentalDecorators": true,
	"preserveConstEnums": true,
	"module": "ESNext",
	"moduleResolution": "bundler",
	"outDir": "./dist/lib",
	"sourceMap": false,
	"strict": true,
	"target": "ESNext",
	"lib": ["ESNext","DOM"],
	"allowJs": true
  }
}